    "version": "0.2.0",
    "configurations": [
        {
            "name": "Thiago",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp2.2/uni-elastic-manager.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "Prometheus": "177.55.119.91:9090",
                "Metric": "wmi_cpu_time_total",
                "Instance": "ubapp01",
                "OS": "windows",
                "CPUThresholdUpper": "80",
                "CPUThresholdLower": "20"
            }
        },
        {
            "name": "Igor",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp2.2/uni-elastic-manager.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "Prometheus": "localhost:9090",
                "Metric": "dockerstats_cpu_usage_ratio",
                "Instance": "prometheus",
                "OS": "linux",
                "Name": "newtoncotes.*",
                "CPUThresholdUpper": "80",
                "CPUThresholdLower": "20",
                "Runnable": "Docker"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}